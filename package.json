{
  "name": "46elks-api-suggestion",
  "private": true,
  "version": "1.0.0",
  "description": "Suggested improved API for 46elks.com",
  "type": "module",
  "main": "./src/server.mjs",
  "scripts": {
    "start": "./src/server.mjs",
    "dev": "nodemon ./src/server.mjs",
    "lint": "prettier --write '{,{src,test}{,/**}/}*.{js,mjs,md}'",
    "test": "run-s lint mocha",
    "mocha": "mocha",
    "npm-audit": "npm install --package-lock-only && npm audit; rm package-lock.json"
  },
  "author": "Hugo Josefson <hugo@josefson.org>",
  "repository": "hugojosefson/46elks-api-suggestion",
  "license": "MIT",
  "mocha": {
    "spec": "test/*test.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js,jsx}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "allow-methods": "^2.0.0",
    "body-parser": "^1.14.2",
    "compose-middleware": "^5.0.1",
    "express": "^4.17.3",
    "git-repo-info": "^2.1.0",
    "load-pkg": "^4.0.0",
    "lodash": "^4.17.19",
    "private-ips": "0.0.3",
    "qs": "^6.9.4",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.7"
  },
  "engines": {
    "node": ">=14"
  }
}
